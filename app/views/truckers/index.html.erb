<h1>Truckers</h1>
<p>List all the truckers !!</p>
<div class="container w-50">
  <table class="table">
    <tr class="header">
      <th>Id </th>
      <th>Trucker Name</th>
    </tr>

    <% @truckers.each do |t| %>
      <tr>
        <td> <%= t.id %></td>
        <td> <%= t.name %></td>
      </tr>
    <% end %>
  </table>
</div>


